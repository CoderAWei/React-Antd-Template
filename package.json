{
  "name": "apanda-fe-template",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!redux-persist)"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.4.5",
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "antd": "^4.24.8",
    "craco-less": "^2.0.0",
    "highcharts": "^10.3.3",
    "highcharts-react-official": "^3.2.0",
    "i18next": "^22.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set PORT=5005 && craco start",
    "build:dev": "dotenv -e .env.development craco build",
    "build:sit": "dotenv -e .env.sit craco build",
    "build:uat": "dotenv -e .env.uat craco build",
    "build:prod": "dotenv -e .env.production craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:stylelint": "stylelint --fix src/**/*.{css,less}",
    "prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "axios": "^1.3.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.0",
    "less": "^4.1.3",
    "less-loader": "^10.2.0",
    "lint-staged": "^13.2.2",
    "postcss-less": "^6.0.0",
    "redux-devtools": "^3.7.0"
  }
}
